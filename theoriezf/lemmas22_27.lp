apply and_i;
apply and_eg _ _ (and_eg _ _ Hy');
apply imp_i (y = y') _ _ _;
assume H2;
apply imp_i (y' = y) _ _ _;
assume H3;
apply H3 (λ w, eta a z w) (and_ed _ _ Hy');
apply lemmaSymEg _ _ H2;
apply lemma9 _ _ (and_ed _ _ (and_eg _ _ Hy'));
assume H4;
apply f_e _ _;
apply ExElim n _ H4 _; 
assume z Hz;
